<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meghagajbhiye.budgetcare_megha">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BudgetCareHome"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".CashIn"
            android:label="@string/title_activity_cash_in" >
        </activity>
        <activity
            android:name=".CashOut"
            android:label="@string/title_activity_cash_out" >
        </activity>
        <activity
            android:name=".Budget"
            android:label="@string/title_activity_budget" >
        </activity>
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".AddCategory"
            android:label="@string/title_activity_add_category" >
        </activity>
        <activity
            android:name=".Statistics"
            android:label="@string/title_activity_statistics" >
        </activity>
        <activity
            android:name=".Overview"
            android:label="@string/title_activity_overview" >
        </activity>


    </application>

</manifest>
